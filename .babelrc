{
  "env": {
    "umd": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "chrome": "64"
            },
            "shippedProposals": true,
            "modules": false
          }
        ]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "external-helpers"
      ]
    },
    "commonjs": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "node": "current"
            }
          }
        ]
      ],
      "plugins": [
        "transform-object-rest-spread",
        [
          "transform-es2015-modules-commonjs",
          {
            "loose": true
          }
        ]
      ]
    },
    "es": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  }
}