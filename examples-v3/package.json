{
  "name": "zetapush-example-v3",
  "version": "0.0.1",
  "main": "server/file.js",
  "private": true,
  "scripts": {
    "start": "zetapush-cli",
    "serve": "serve public/",
    "worker:start": "pm2 start zetapush-cli -i max",
    "worker:stop": "pm2 stop all"
  },
  "dependencies": {
    "node": "8",
    "zetapush-cli": "../zetapush-cli",
    "zetapush-js": "3.4.0-alpha.8"
  },
  "zetapush": {
    "apiUrl": "http://hq.zpush.io:9080/zbo/pub/business",
    "sandboxId": "<%= sandboxId %>",
    "login": "<%= login %>",
    "password": "<%= password %>"
  },
  "devDependencies": {
    "pm2": "^2.9.1",
    "serve": "^6.4.9"
  }
}